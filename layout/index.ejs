	<div class="row" id="content">
		<div class="container">
			<div class="col s12 m12" style="margin:0 auto">
				<div id="art-container">
					<% page.posts.each(function(post){ %>
					<div class="article-body">
						<div class="article-tag">
							<div class="article-title">
								<a href="/<%- post.path %>">
								<h2><%= post.title %></h2>
								</a>
								<div class="link-border">
								</div>
							</div>
							<div class="left article-info">
           <p><i class="fa fa-tags"></i><%- list_tags(post.tags) %></p>
           <p><%- date(post.date) %></p>
          </div>
							</div>
						<div class="article-background" style="background-image:url()">
						</div>
						<div class="article-content">
							<% if (post.excerpt){ %>
								<%- post.excerpt %>
								<p>
									<a href="<%- post.path %>">阅读全文...</a>
								</p>
							<% } else { %>
								<% var content = strip_html(post.content) %>
								<%- content.substring(0, 65) %>
							<% } %>
						</div>
					</div>
					<% }) %>
					<% if (page.total > 1){ %>
						<p>
						<nav class="pagination">
						<!-- nav id="page-nav" -->
						<%- paginator({
							
        prev_text: '<i class="fa fa-angle-left"></i>',
        next_text: '<i class="fa fa-angle-right"></i>',
        mid_size: 1
						}) %>
						</p>
						</nav>
					<% } %>
					<!-- p>{{pagination}}</p -->
				</div>
			</div>
		</div>
	</div>
